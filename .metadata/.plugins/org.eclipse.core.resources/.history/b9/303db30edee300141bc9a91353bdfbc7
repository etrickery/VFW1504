//TABLE VIEW

//var cars = Ti.UI.createTableView({
//	data: "fiat"
//});




for(var make in mopar){
	for(var a in mopar[make]){
		if(a === "cars"){
			for(var b=0; b<mopar[make].cars.length; b++){
				for(var c in mopar[make].cars[b]){
					if(c === "model"){
							var menuRow = Ti.UI.createTableViewRow({
								model: mopar[make].cars[b][c]
								
							});
							console.log(mopar[make].cars[b][c]);
							menuText.add(menuRow);
					}
				}
			}
		}
	}
}
